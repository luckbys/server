{
  "name": "evolution-webhook-server",
  "version": "2.0.0",
  "description": "Servidor de webhook escalável compatível com Evolution API",
  "main": "start-production.js",
  "scripts": {
    "build": "tsc",
    "start": "node start-production.js",
    "start:prod": "NODE_ENV=production node start-production.js",
    "start:simple": "node start-simple.js",
    "dev": "node start-simple.js",
    "test": "jest",
    "test:evolution": "node test-evolution-compatibility.js",
    "migrate": "ts-node src/database/migrations/run.ts",
    "docker:build": "docker build -t evolution-webhook-server .",
    "docker:run": "docker run -p 3001:3001 evolution-webhook-server"
  },
  "keywords": [
    "express",
    "whatsapp",
    "evolution-api",
    "webhook",
    "typescript",
    "rabbitmq",
    "postgresql",
    "redis",
    "scalable"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.4",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "winston": "^3.11.0",
    "axios": "^1.6.7",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "amqplib": "^0.10.3",
    "zod": "^3.22.4",
    "uuid": "^9.0.1",
    "compression": "^1.7.4",
    "rate-limiter-flexible": "^4.0.1",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/pg": "^8.10.7",
    "@types/amqplib": "^0.10.4",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.3.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "supertest": "^6.3.4"
  }
} 